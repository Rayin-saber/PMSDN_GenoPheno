	author = {Wilson, H. L. and Wong, A. C. C. and Shaw, S. R. and Tse, W.-Y. and Stapleton, G. A. and Phelan, M. C. and Hu, S. and Marshall, J. and {McDermid}, H. E.},
	date = {2003-08},
