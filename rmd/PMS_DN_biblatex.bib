	number = {4},
	journaltitle = {{PloS} One},
	shortjournal = {{PLoS} {ONE}},
	author = {van Eerde, Albertien M. and Duran, Karen and van Riel, Els and de Kovel, Carolien G. F. and Koeleman, Bobby P. C. and Knoers, Nine V. A. M. and Renkema, Kirsten Y. and van der Horst, Henricus J. R. and Bökenkamp, Arend and van Hagen, Johanna M. and van den Berg, Leonard H. and Wolffenbuttel, Katja P. and van den Hoek, Joop and Feitz, Wouter F. and de Jong, Tom P. V. M. and Giltay, Jacques C. and Wijmenga, Cisca},
	date = {2012},
	pmid = {22558067},
	pmcid = {PMC3338743},
	keywords = {Case-Control Studies, Genetic Association Studies, Genetic Variation, Genotype, Humans, Intercellular Signaling Peptides and Proteins, Intracellular Signaling Peptides and Proteins, Linkage Disequilibrium, Morphogenesis, Netherlands, Nuclear Proteins, Polymorphism, Single Nucleotide, Protein Tyrosine Phosphatases, Receptors, Immunologic, Transforming Growth Factor beta1, Ureter, Uroplakin {III}, Vesico-Ureteral Reflux}
