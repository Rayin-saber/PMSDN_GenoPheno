	shortjournal = {Nucleic Acids Res.},
	author = {Medina, Ignacio and Salavert, Francisco and Sanchez, Rubén and de Maria, Alejandro and Alonso, Roberto and Escobar, Pablo and Bleda, Marta and Dopazo, Joaquín},
	date = {2013-07},
	pmid = {23748955},
	pmcid = {PMC3692043},
	keywords = {Genome, Genomics, Internet, Software}
}

@article{sarasua_association_2011,
	title = {Association between deletion size and important phenotypes expands the genomic region of interest in Phelan–{McDermid} syndrome (22q13 deletion syndrome)},
	volume = {48},
	issn = {, 1468-6244},
	url = {http://jmg.bmj.com.ezp-prod1.hul.harvard.edu/content/48/11/761},
	doi = {10.1136/jmedgenet-2011-100225},
	abstract = {Background The clinical features of Phelan–{McDermid} syndrome (also known as 22q13 deletion syndrome) are highly variable and include hypotonia, speech and other developmental delays, autistic traits and mildly dysmorphic features. Patient deletion sizes are also highly variable, prompting this genotype–phenotype association study.
Methods Terminal deletion breakpoints were identified for 71 individuals in a patient cohort using a custom-designed high-resolution oligonucleotide array comparative genomic hybridisation platform with a resolution of 100 bp.
Results Patient deletion sizes were highly variable, ranging from 0.22 to 9.22 Mb, and no common breakpoint was observed. {SHANK}3, the major candidate gene for the neurologic features of the syndrome, was deleted in all cases. Sixteen features (neonatal hypotonia, neonatal hyporeflexia, neonatal feeding problems, speech/language delay, delayed age at crawling, delayed age at walking, severity of developmental delay, male genital anomalies, dysplastic toenails, large or fleshy hands, macrocephaly, tall stature, facial asymmetry, full brow, atypical reflexes and dolichocephaly) were found to be significantly associated with larger deletion sizes, suggesting the role of additional genes or regulatory regions proximal to {SHANK}3. Individuals with autism spectrum disorders ({ASDs}) were found to have smaller deletion sizes (median deletion size of 3.39 Mb) than those without {ASDs} (median deletion size 6.03 Mb, p=0.0144). This may reflect the difficulty in diagnosing {ASDs} in individuals with severe developmental delay.
Conclusions This genotype–phenotype analysis explains some of the phenotypic variability in the syndrome and identifies new genomic regions with a high likelihood for causing important developmental phenotypes such as speech delay.},
	pages = {761--766},
	number = {11},
	journaltitle = {Journal of Medical Genetics},
	shortjournal = {J Med Genet},
	author = {Sarasua, Sara M. and Dwivedi, Alka and Boccuto, Luigi and Rollins, Jonathan D. and Chen, Chin-Fu and Rogers, R. Curtis and Phelan, Katy and {DuPont}, Barbara R. and Collins, Julianne S.},
	urldate = {2015-02-02},
	date = {2011-11-01},
	langid = {english},
	pmid = {21984749},
	keywords = {22q13 deletion syndrome, autism, copy-number, cytogenetics, developmental, diabetes, drugs: endocrine system, genetic epidemiology, genotype–phenotype, immunology (including allergy), metabolic disorders, microarray, molecular genetics, neurosciences, obesity, genetics, other endocrinology, Phelan–{McDermid} syndrome, speech and language},
	file = {Full Text PDF:/home/maxx/.mozilla/firefox/iqcphlk1.default/zotero/storage/T2P7B45E/Sarasua et al. - 2011 - Association between deletion size and important ph.pdf:application/pdf;Snapshot:/home/maxx/.mozilla/firefox/iqcphlk1.default/zotero/storage/PITNTXZB/761.html:text/html}
}

